class Solution {
    public void rotate(int[][] matrix) {
        int n = matrix.length - 1;
        for(int i=0;i<n+1;i++){
            for (int j=i;j<n+1;j++){
               int temp = matrix[j][i];
                matrix[j][i] = matrix[i][j];
                matrix[i][j] = temp;
            }
        }
        
        for(int i=0;i<n+1;i++){
            int start = 0;int end = n;
            while(start<end){
                int temp = matrix[i][start];
                matrix[i][start] = matrix [i][end];
                matrix [i][end] = temp;
                start++;end--;
            }
            
        }
        
         for(int i=0;i<n+1;i++){
            for (int j=0;j<n+1;j++){
                System.out.print(matrix[i][j]);
            }
             System.out.println("");
        }
        
    }
}
