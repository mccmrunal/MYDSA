/**
 * @param {number[]} nums
 * @return {number}
 */
var pivotIndex = function(nums) {
    let prefixSum = [];
    prefixSum[0] = nums[0];
    let suffixSum = [];
    suffixSum[0] = nums[nums.length-1];
    for(let i=1;i<nums.length;i++){
        let suffixIndex = (nums.length-1)-i;

        suffixSum[i] = suffixSum[i-1] + nums[suffixIndex];
        prefixSum[i] = prefixSum[i-1] + nums[i]
    };
    console.log(prefixSum,suffixSum)

        for(let i=0;i<nums.length;i++){
            console.log(prefixSum[i] , suffixSum[(nums.length-1)-i])
            if(prefixSum[i] === suffixSum[(nums.length-1)-i]){
                return i
            }
    };

return -1

};
